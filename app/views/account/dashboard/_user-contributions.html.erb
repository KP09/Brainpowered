<div class="project-row flex-between">
    <div>
      <p><%= contribution.project.title[0..30] %>...</p>
    </div>
    <div>
      <p><%= contribution.project.user.company_name %></p>
    </div>
    <div>
      <p class="answer-col"><%= contribution.starred? %></p>
    </div>
    <div>
      <p><%= link_to "View", cl_image_path(contribution.file.path), class: "btn btn-primary padding-none full-width-button" %></p>
    </div>
</div>
